;Файл был сгенерирован сервером puppet

[DiscountCounters]
; Настройки счетчиков

; по умолчанию выключены (enable = false)
enable = <%= @discountcounters_enable %>

; Имя хоста
host = <%= @discountcounters_host %>

; Номер порта
port = <%= @discountcounters_port %>

; Таймаут ожидания ответа от сервера
timeout = <%= @discountcounters_timeout %>

; Логин
user = <%= @discountcounters_user %>

; Пароль
password = <%= @discountcounters_password %>
