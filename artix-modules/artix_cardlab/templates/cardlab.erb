; Файл был сгенерирован сервером puppet
[plugins]
; Система лояльности CardLab
cardlab = <%= cardlab_enable %>

[CardLab]
; URL сервера
serverUrl = <%= @cardlab_serverUrl %>

; Логин для подключения к серверу
login = <%= @cardlab_login %>

; Пароль для подключения к серверу
password = <%= @cardlab_password %>

