;-- Файл был сгенерирован сервером puppet

[plugins] ; Система лояльности Loya
loya = <%= @loya_enable %>

[Loya]
; Настройки плагина системы лояльности Loya
; Адрес сервера Loya
; Значение по умолчанию http://localhost.
serverUrl = <%= @loya_serverUrl %>

; Таймаут запроса к серверу Loya(в секундах)
; Значение по умолчанию 30
timeout = <%= @loya_timeout %>

; Идентификатор терминала (кассы)
; Значение по умолчанию ""
pos = <%= @loya_pos %>

; Код магазина
; Значение по умолчанию ""
merchantId = <%= @loya_merchantId %>

; Имя пользователя для авторизации на сервере Loya
; Значение по умолчанию ""
user = <%= @loya_user %>

; Пароль пользователя для авторизации на сервере Loya
; Значение по умолчанию ""
password = <%= @loya_password %>

; Api-ключ приложения
; Значение по умолчанию ""
apikey = <%= @loya_apikey %>