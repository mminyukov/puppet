[NetworkTester]
; Настройки для проверки доступа к удаленнному ресурсу

; интервал проверки (в секундах), по умолчанию 30 секунд
timeout = <%= @networktest_timeout %>

; хост
host = <%= @networktest_host %>

; Порт. Если порт не задан, то проверка доступа будет осуществляться только по хосту
port = <%= @networktest_port %>

