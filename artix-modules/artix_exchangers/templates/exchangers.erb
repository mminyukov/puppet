# Defaults for the "exchangers-unload" and "exchangers-upload" initscript

# Set to yes to start exchangers-unload.
UNLOAD_START=<% if @serviceUnload %>yes<% else %>no<% end %>

# Set to yes to start exchangers-upload
UPLOAD_START=<% if @serviceUpload %>yes<% else %>no<% end %>

# Set to yes to start exchangers-soap-upload
UPLOAD_SOAP_START=<% if @serviceSoapUpload %>yes<% else %>no<% end %>

# Set to yes to start exchangers-rest-upload
UPLOAD_REST_START=<% if @serviceRestUpload %>yes<% else %>no<% end %>

